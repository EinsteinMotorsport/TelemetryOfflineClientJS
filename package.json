{
  "name": "offlineclient",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "d3": "^5.7.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-mosaic-component": "^2.0.2",
    "react-redux": "^5.1.1",
    "react-scripts": "2.1.1",
    "styled-components": "^4.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": [
        "warn",
        "never",
        {
          "beforeStatementContinuationChars": "always"
        }
      ],
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ],
      "quotes": [
        "warn",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
