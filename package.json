{
  "name": "offlineclient",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^10.12.23",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "d3": "^5.9.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-mosaic-component": "^2.0.2",
    "react-redux": "^5.1.1",
    "react-scripts": "^2.1.3",
    "styled-components": "^4.1.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "npx serve -s build"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": [
        "warn",
        "never",
        {
          "beforeStatementContinuationChars": "always"
        }
      ],
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ],
      "quotes": [
        "warn",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
